mport \"./Product.scss\";\nimport AddShoppingCartIcon from \"@mui/icons-material/AddShoppingCart\";\nimport FavoriteBorderIcon from \"@mui/icons-material/FavoriteBorder\";\nimport BalanceIcon from \"@mui/icons-material/Balance\";\nimport useFetch from \"../../hooks/useFetch\";\nimport { useParams } from \"react-router-dom\";\nimport { useDispatch } from \"react-redux\";\nimport { addToCart } from \"../../redux/cartReducer\";\n\nconst Product = () => {\n  const id = useParams().id;\n  const [selectedImg, setSelectedImg] = useState(\"img\");\n  const [quantity, setQuantity] = useState(1);\n\n  const dispatch = useDispatch();\n  const { data, loading, error } = useFetch(`/products/${id}?populate=*`);\n\n  return (\n    <div className=\"product\">\n      {loading ? (\n        \"loading\"\n      ) : (\n        <>\n          <div className=\"left\">\n            <div className=\"images\">\n              <img\n                src={\n                  process.env.REACT_APP_UPLOAD_URL +\n                  data?.attributes?.img?.data?.attributes?.url\n                }\n                alt=\"\"\n                onClick={(e) => setSelectedImg(\"img\")}\n              />\n              <img\n                src={\n                  process.env.REACT_APP_UPLOAD_URL +\n                  data?.attributes?.img2?.data?.attributes?.url\n                }\n                alt=\"\"\n                onClick={(e) => setSelectedImg(\"img2\")}\n              />\n            </div>\n            <div className=\"mainImg\">\n              <img\n                src={\n                  process.env.REACT_APP_UPLOAD_URL +\n                  data?.attributes[selectedImg]?.data?.attributes?.url\n                }\n                alt=\"\"\n              />\n            </div>\n          </div>\n          <div className=\"right\">\n            <h1>{data?.attributes?.title}</h1>\n            <span className=\"price\">${data?.attributes?.price}</span>\n            <p>{data?.attributes?.desc}</p>\n            <div className=\"quantity\">\n              <button\n                onClick={() =>\n                  setQuantity((prev) => (prev === 1 ? 1 : prev - 1))\n                }\n              >\n                -\n              </button>\n              {quantity}\n              <button onClick={() => setQuantity((prev) => prev + 1)}>+</button>\n            </div>\n            <button\n              className=\"add\"\n              onClick={() =>\n                dispatch(\n                  addToCart({\n                    id: data.id,\n                    title: data.attributes.title,\n                    desc: data.attributes.desc,\n                    price: data.attributes.price,\n                    img: data.attributes.img.data.attributes.url,\n                    quantity,\n                  })\n                )\n              }\n            >\n              <AddShoppingCartIcon /> ADD TO CART\n            </button>\n            <div className=\"links\">\n              <div className=\"item\">\n                <FavoriteBorderIcon /> ADD TO WISH LIST\n              </div>\n              <div className=\"item\">\n                <BalanceIcon /> ADD TO COMPARE\n              </div>\n            </div>\n            <div className=\"info\">\n              <span>Vendor: Polo</span>\n              <span>Product Type: T-Shirt</span>\n              <span>Tag: T-Shirt, Women, Top</span>\n            </div>\n            <hr />\n            <div className=\"info\">\n              <span>DESCRIPTION</span>\n              <hr />\n              <span>ADDITIONAL INFORMATION</span>\n              <hr />\n              <span>FAQ</span>\n            </div>\n          </div>\n        </>\n      )}\n    </div>\n  );\n};\n\nexport default Product;","/home/dagim/Desktop/YohannesEcommerce/client/src/pages/Products/Products.jsx",[],[],"/home/dagim/Desktop/YohannesEcommerce/client/src/components/Footer/Footer.jsx",[],[],"/home/dagim/Desktop/YohannesEcommerce/client/src/components